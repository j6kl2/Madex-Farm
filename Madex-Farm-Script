StopNAL.MouseButton1Click:Connect(function()
	StopNAL.Visible = false
	NAL.Visible = true
	for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if v:IsA('MeshPart') then v.Anchored = false
		end
	end
end)

